  },
  "keywords": [
    "apm",
    "prometheus",
    "middleware",
    "latency",
    "handles",
    "eventloop",
    "gc",
    "cpu",
    "memory"
  ],
  "homepage": "https://github.com/VoodooTeam/prometheus-middleware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VoodooTeam/prometheus-middleware.git"
  },
  "scripts": {
